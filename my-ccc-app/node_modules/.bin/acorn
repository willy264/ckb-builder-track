#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/node_modules:/mnt/c/Users/Engr. Williams/Documents/node_modules:/mnt/c/Users/Engr. Williams/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Engr. Williams/documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/bin/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules/acorn/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/acorn@8.15.0/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/ckb-builder-track/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/web3bridge/node_modules:/mnt/c/Users/Engr. Williams/Documents/people2/node_modules:/mnt/c/Users/Engr. Williams/Documents/node_modules:/mnt/c/Users/Engr. Williams/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Engr. Williams/documents/people2/web3bridge/ckb-builder-track/my-ccc-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../.pnpm/acorn@8.15.0/node_modules/acorn/bin/acorn" "$@"
fi
